package LabTest3;

import java.util.Arrays;
import java.util.HashSet;
import java.util.Scanner;

public class Question1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		  Scanner scan = new Scanner (System.in);
		 String [] item = new String [5];
		 String [] item2 = new String [5];
		 String [] item3 = new String [5];
		 
		 Input (item);
		 Input (item2);
		 boolean delete=deleteValue(item,item2);
	     
		 Count (item);
		 Count (item2);
	}


	public static void Input (String [] i) {
		Scanner scan = new Scanner (System.in);
		System.out.println("Please input 5 words (String).");
		for (int q = 0; q < i.length;q++)  
		i[q] = scan.nextLine();
	}
	
	public static boolean deleteValue(String[] x, String[] y) {
		// TODO Auto-generated method stub
		int i,j;
	    boolean delete=false;
	    for (i=0; i < x.length; i++) {
	      for(j=i;j<y.length-1;j++)
	     if(x[i] == y[j]){
	         x[j]=x[j+1];
	      delete=true;
	      break;
	      }
	    }
	    if (delete==false)
	        System.out.println("The search value is not in the list.");
	        return delete;  
	    
	}
	public static void Count (String [] i) {
		for (int q = 0; q < i.length;q++)  
	   System.out.println("The length of the word is " + i[q].length());
	}
	
	
	
}
